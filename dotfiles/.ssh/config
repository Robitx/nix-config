# DevPod Start async-scanner.devpod
Host async-scanner.devpod
  ForwardAgent yes
  LogLevel error
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  HostKeyAlgorithms rsa-sha2-256,rsa-sha2-512,ssh-rsa
  ProxyCommand "/nix/store/8smwfvb8950zrj9r4va9vi5cc8pwn2ry-devpod-0.5.16/bin/devpod" ssh --stdio --context default --user root async-scanner
  User root
# DevPod End async-scanner.devpod

